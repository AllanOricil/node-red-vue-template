{
  "name": "node-red-vue-template",
  "version": "1.2.1",
  "description": "A template to build Node-RED nodes using Vue 3 and Typescript",
  "author": "Allan Oricil <allanoricil@duck.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/AllanOricil/node-red-vue-template",
    "type": "git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10.11.0"
  },
  "keywords": [
    "nrg",
    "template",
    "vite",
    "vue"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:client:dev": "NODE_ENV=development TARGET=client tsx build.ts",
    "build:server:dev": "NODE_ENV=development TARGET=server tsx build.ts",
    "build:client": "TARGET=client tsx build.ts",
    "build:server": "TARGET=server tsx build.ts",
    "build:dev": "NODE_ENV=development tsx build.ts",
    "build": "tsx build.ts",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "tsc:client": "tsc -p client/tsconfig.json",
    "tsc:server": "tsc -p server/tsconfig.json"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@types/express": "^5.0.1",
    "@types/jquery": "^3.5.32",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.15.18",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "esbuild": "^0.25.4",
    "esbuild-plugin-tsc": "^0.5.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.1.0",
    "fs-extra": "^11.3.0",
    "globals": "^16.1.0",
    "mime-types": "^3.0.1",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "semantic-release": "^24.2.4",
    "tsx": "^4.19.4",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.4"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.33",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "es-toolkit": "^1.37.2",
    "jquery": "^3.7.1",
    "jsonpointer": "^5.0.1",
    "vue": "^3.5.14"
  }
}
